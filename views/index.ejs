                        <button type="button" data-bs-target="#Indicatordot" data-bs-slide-to="2" aria-label="Slide 3"
                            class="rounded-circle active" aria-current="true"></button>
                    </div>
                    <div class="carousel-inner">
                        <div class="carousel-item" data-bs-interval="10000">
                            <img src="./assets/img/slide1-car.png" class="d-block w-100" alt="slide 1">
                        </div>
                        <div class="carousel-item" data-bs-interval="10000">
